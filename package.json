{
  "name": "call-gpt",
  "version": "2.0.0",
  "description": "Twilio voice AI application with OpenAI GPT-4 and Deepgram for phone call automation",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "lint": "eslint src/",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit",
    "inbound": "node dist/scripts/inbound-call.js",
    "outbound": "node dist/scripts/outbound-call.js"
  },
  "keywords": [
    "twilio",
    "openai",
    "deepgram",
    "voice-ai",
    "phone-automation"
  ],
  "author": "daniel.w@eorumyoung.com",
  "license": "MIT",
  "dependencies": {
    "@deepgram/sdk": "^3.3.4",
    "axios": "^1.6.0",
    "colors": "^1.4.0",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "express-ws": "^5.0.2",
    "openai": "^4.20.1",
    "twilio": "^4.19.3",
    "uuid": "^9.0.1",
    "wavefile": "^11.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18"
  }
}
